/* 顶部样式 */
swiper image{
  width: 100%;
  height: 300rpx;
}
.content{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.content-item{
  width: 20%;
  text-align: center;
  font-size: 24rpx;
  margin: 8rpx 0;
}
.hr{
  width: 100%;
  height: 30rpx;
  background-color: #f4f5f6;
}
.mid{
  display: flex;
  flex-direction: row;
  padding: 40rpx;
}
.footer{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 40rpx;
}
text{
  font-weight: bold;
}